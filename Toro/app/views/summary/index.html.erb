<table class="table table-bordered" style="background-color:white;">
<tr>
  <th>#</th>
  <th>Date</th>
  <th>Shift</th>
  <th>Time</th>
  <th>Employee</th>
  <th>Cheque Price</th>
</tr>
<% @check.each do |check| %>
    <tr>
      <td><%= check.id %></td>
      <td><%= check.date %></td>
      <td><%= check.shift %></td>
      <td><%= check.datetime %></td>
      <td><%= check.employee %></td>
      <td><%= check.posttax %></td>
    </tr>
  <% end %> 
</table>