<%= link_to "<<Back", users_path%>
<br>
<h2>Edit user</h2>
<%= form_tag("/users/#{@user.id}/update", :method => "post") do %>
  <%= label_tag(:q, "New password:") %>
  <%= password_field_tag(:p) %>
  <%= label_tag(:q, "New password confirmation:") %>
  <%= password_field_tag(:pc) %>
  <br>
  <%= submit_tag "Update", class: "btn btn-success btn-large"%>
<% end %>